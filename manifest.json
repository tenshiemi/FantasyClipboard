{
  "manifest_version": 2,

  "name": "Fantasy Clipper",
  "version": "1.2",
  "description": "Copy your Fantasy players to the clipboard.",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },

  "permissions": [
    "http://football.fantasysports.yahoo.com/",
    "clipboardRead",
    "clipboardWrite"
    ]
}
