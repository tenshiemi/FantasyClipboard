{
  "manifest_version": 2,
  "name": "Fantasy Clipper",
  "version": "2.0.1",
  "description": "Copy your Fantasy players to the clipboard.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  "permissions": [
    "http://football.fantasysports.yahoo.com/",
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "tabs"
    ]
}
